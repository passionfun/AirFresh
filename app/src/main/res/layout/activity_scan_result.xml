<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context="com.example.bonfeel.perfumediffuser_beifen_chineseappanv311.activity.ScanResultActivity" >

    <com.example.bonfeel.perfumediffuser_beifen_chineseappanv311.custom.TitleBar
    	android:id="@+id/titleBar_scanResult"
    	android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:layout_alignParentTop="true"
    	android:layout_centerHorizontal="true" >
	</com.example.bonfeel.perfumediffuser_beifen_chineseappanv311.custom.TitleBar>

    <TextView
    	android:id="@+id/tv_result_tag"
    	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_below="@+id/titleBar_scanResult"
    	android:layout_centerHorizontal="true"
    	android:layout_marginTop="24dp"
    	android:textSize="20sp"
    	android:text="二维码内容如下：" />

    <TextView
    	android:id="@+id/tv_spinder"
    	android:layout_width="match_parent"
    	android:layout_height="1dp"
    	android:layout_below="@+id/tv_result_tag"
    	android:layout_centerHorizontal="true"
    	android:layout_marginLeft="16dp"
    	android:layout_marginRight="16dp"
    	android:background="#cccccc" />

    <TextView
    	android:id="@+id/tv_scan_result"
    	android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:layout_below="@+id/tv_spinder"
    	android:layout_centerHorizontal="true"
    	android:layout_marginTop="16dp"
    	android:layout_marginLeft="16dp"
    	android:layout_marginRight="16dp"
    	android:gravity="center"
    	android:textSize="20sp"
    	android:text="" />

</RelativeLayout>
