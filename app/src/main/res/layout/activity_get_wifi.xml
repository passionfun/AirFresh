<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:paddingBottom="@dimen/activity_vertical_margin"
	tools:context="com.example.bonfeel.perfumediffuser_beifen_chineseappanv311.activity.GetWifiActivity" >

	<com.example.bonfeel.perfumediffuser_beifen_chineseappanv311.custom.TitleBar
    	android:id="@+id/titleBar_wifi_list"
    	android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:layout_alignParentTop="true"
    	android:layout_centerHorizontal="true">
	</com.example.bonfeel.perfumediffuser_beifen_chineseappanv311.custom.TitleBar>
    
	 <ListView
    	android:id="@+id/lv_wifi"
    	android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_below="@+id/titleBar_wifi_list"
		android:layout_centerHorizontal="true"
		android:layout_marginTop="16dp"
		android:layout_marginLeft="16dp"
		android:layout_marginRight="16dp" >
    </ListView>

</RelativeLayout>
